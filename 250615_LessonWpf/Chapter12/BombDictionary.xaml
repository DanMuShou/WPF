<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <CombinedGeometry x:Key="BombGeometry">
        <CombinedGeometry.Geometry1>
            <CombinedGeometry>
                <CombinedGeometry.Geometry1>
                    <EllipseGeometry
                        Center="20 20"
                        RadiusX="20"
                        RadiusY="20" />
                </CombinedGeometry.Geometry1>
                <CombinedGeometry.Geometry2>
                    <EllipseGeometry
                        Center="5 0"
                        RadiusX="5"
                        RadiusY="7">
                        <EllipseGeometry.Transform>
                            <TransformGroup>
                                <RotateTransform Angle="45" />
                            </TransformGroup>
                        </EllipseGeometry.Transform>
                    </EllipseGeometry>
                </CombinedGeometry.Geometry2>
            </CombinedGeometry>
        </CombinedGeometry.Geometry1>
        <CombinedGeometry.Geometry2>
            <RectangleGeometry
                RadiusX="2"
                RadiusY="2"
                Rect=" 0 0 10 13">
                <RectangleGeometry.Transform>
                    <TransformGroup>
                        <TranslateTransform X="4" Y="-6" />
                        <RotateTransform Angle="45" />
                    </TransformGroup>
                </RectangleGeometry.Transform>
            </RectangleGeometry>
        </CombinedGeometry.Geometry2>
    </CombinedGeometry>
</ResourceDictionary>